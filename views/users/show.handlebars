<header class="page-header">
  <h1>User: {{user.fname}} {{user.lname}}</h1>
</header>

<table class="table">
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>First Name</td>
      <td>{{user.fname}}</td>
    </tr>
    <tr>
      <td>Last Name</td>
      <td>{{user.lname}}</td>
    </tr>
    <tr>
      <td>Email</td>
      <td>{{user.email}}</td>
    </tr>
    <tr>
      <td>API Token</td>
      <td>{{user.token}}</td>
    </tr>
  </tbody>
</table>
